{

  "name": "Show if link is target _blank",
  "description": "Shows you if the targeted link has an _blank tag.",
  "manifest_version": 2,
  "version": "1.0",
  "homepage_url": "https://joel.cool",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["blank.js"],
      "css": ["blank.css"]
    }
  ]

}